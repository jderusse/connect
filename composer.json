{
    "name": "symfonycorp/connect",
    "description": "SymfonyConnect SDK",
    "keywords": ["SymfonyConnect", "SDK"],
    "homepage": "https://github.com/symfonycorp/connect",
    "type": "library",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=7.1.3",
        "ext-dom": "*",
        "ext-json": "*",
        "psr/log": "^1.0",
        "symfony/http-client": "^4.4|^5.0"
    },
    "require-dev": {
        "symfony/form": "^4.4|^5.0",
        "symfony/security-bundle": "^4.4|^5.2",
        "symfony/phpunit-bridge": "^4.4|^5.0"
    },
    "conflict": {
        "symfony/security-core": "<4.3",
        "symfony/security-bundle": "<5.2"
    },
    "autoload": {
        "psr-4": {
            "SymfonyCorp\\Connect\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyCorp\\Connect\\Test\\": ["tests/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.x-dev"
        }
    }
}
