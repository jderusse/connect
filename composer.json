{
    "name": "symfonycorp/connect",
    "description": "SymfonyConnect SDK",
    "keywords": ["SymfonyConnect", "SDK"],
    "homepage": "https://github.com/symfonycorp/connect",
    "type": "library",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "psr/log": "^1.0",
        "symfony/http-client": "^4.4"
    },
    "require-dev": {
        "symfony/form": "^2.8|^3.4|^4.4",
        "symfony/security": "^2.8|^3.4|^4.4",
        "symfony/phpunit-bridge": "^4.4"
    },
    "suggest": {
        "symfony/security": "To use in a Symfony environment"
    },
    "autoload": {
        "psr-4": {
            "SymfonyCorp\\Connect\\": ["src/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
